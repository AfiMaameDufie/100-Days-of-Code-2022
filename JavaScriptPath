function abTest(a, b) {
  if (a < 0 | b < 0) {
    return undefined;
  }

  // Only change code below this linee

  // Only change code above this line

  return Math.round(Math.pow(Math.sqrt(a) + Math.sqrt(b), 2));
}

abTest(2,2);



Array.prototype.myMap = function(callback) {
  const newArray = [];
  // Only change code below this line
  for(let i=0; i < this.length; i++) {
    newArray.push(callback(this[i]));
  }
  // Only change code above this line
  return newArray;
};

const new_s = s.myMap(function(item) {
  return item * 2;
});



